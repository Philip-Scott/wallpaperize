<component type="desktop">
  <id>com.github.philip-scott.wallpaperize</id>
  <metadata_license>CC0-1.0</metadata_license>
  <project_license>GPL-3.0+</project_license>
  <name>Wallpaperize</name>
  <summary>Beautify your oddly-sized wallpapers</summary>
  <description>
    <p>
      Turn your oddly sized wallpapers into a piece of beauty. Simply right click on your picture's file, and select "Wallpaperize" from the context menu, or open the app and drag-and-drop the images you wish to wallpaperize.
    </p>
  </description>
  <provides>
    <binary>com.github.philip-scott.wallpaperize</binary>
  </provides>
  <releases>
    <release version="1.2.8" date="2019-05-05">
      <description>
        <p>Juno Release</p>
        <ul>
          <li>Wallpaperize is now available in elementary OS Juno!</li>
        </ul>
      </description>
    </release>
  </releases>
  <screenshots>
    <screenshot type="default">
